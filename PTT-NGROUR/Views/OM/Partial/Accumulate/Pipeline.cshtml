@model PTT_NGROUR.Models.ViewModel.ModelOmIndex

@{
    List<PTT_NGROUR.Models.DataModel.ModelAccumulatedResults> data = Model.Accumulated.Pipeline;
}

<style>
    .xaxis line {
        fill: none;
        stroke: none;
    }

    .monthaxis text {
        fill: #a4a4a4;
    }

    .monthaxis .domain {
        display: none;
    }
</style>


@if (data.Count() > 0)
{
    <div id="chartWrapperAccumulatePipeline" class="chart-wrapper">
        <div id="chartAccumulatePipeline" class="chart"></div>
    </div>
}