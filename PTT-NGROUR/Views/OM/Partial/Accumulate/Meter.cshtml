@model PTT_NGROUR.Models.ViewModel.ModelOmIndex

@{
    List<PTT_NGROUR.Models.DataModel.ModelAccumulatedResults> data = Model.Accumulated.Meter;
}

<style>
    .xaxis line {
        fill: none;
        stroke: none;
    }

    .monthaxis text {
        fill: #a4a4a4;
    }

    .monthaxis .domain {
        display: none;
    }
</style>


@if (data.Count() > 0)
{
    <div id="chartWrapperAccumulateMeter" class="chart-wrapper">
        <div id="chartAccumulateMeter" class="chart"></div>
    </div>
}