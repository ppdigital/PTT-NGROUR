        var fileData = new FormData();

        for (var k = 0; k < files.length; k++) {
            fileData.append(files.name, files[k]);
        }

      var Year = $('#seYear').val();
      var Month = $('#seMonth').val();
      var Region = $('#seRegion').val();
      var Type = $('#seType').val();
